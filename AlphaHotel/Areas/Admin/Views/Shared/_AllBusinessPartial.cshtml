@model ICollection<BusinessDTO>

<div id="admin-dynamic">
    <hr />
    <h2>Choose location!</h2>
    <div class="col-sm-10" id="choose-location">
        @foreach (var business in Model)
        {
            <div class="form-check">
                <input name="BusinessId" type="radio" class="form-check-input business-name" data-url="/admin/business/businessLogbooks/@business.BusinessId" value="@business.BusinessId">
                <label for="BusinessId" class="form-check-label">@business.Name</label>
                @*<span asp-validation-for="@business.BusinessId" class="text-danger"></span>*@
            </div>
        }
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}